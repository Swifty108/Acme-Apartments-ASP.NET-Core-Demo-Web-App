
@{
    ViewData["Title"] = "Gallery";
}

<partial name="_TopNavStatic" />

<div class="index-main-div text-center">

    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel" style="margin-left: 160px; margin-right: auto; display: block;">

        <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
        <div class="carousel-inner">
            <div class="carousel-item">
                <img src="~/images/apt floor plan.jpg" class="d-block w-50" alt="...">
            </div>
            <div class="carousel-item">
                <img src="~/images/thumbNailer (1).jpg" class="d-block w-30" alt="...">
            </div>
            <div class="carousel-item">
                <img src="~/images/thumbNailer (2).jpg" class="d-block w-30" alt="...">
            </div>
            <div class="carousel-item active">
                <img src="~/images/thumbNailer (3).jpg" class="d-block w-30" alt="...">
            </div>
            <div class="carousel-item">
                <img src="~/images/thumbNailer (4).jpg" class="d-block w-30" alt="...">
            </div>
            <div class="carousel-item">
                <img src="~/images/thumbNailer (5).jpg" class="d-block w-30" alt="...">
            </div>
        </div>

    </div>

</div>

